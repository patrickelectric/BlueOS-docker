{
  "dockers": {
    "supervisor": {
      "tag": "latest",
      "image": "bluerobotics/supervisor",
      "enabled": true,
      "webui": false,
      "network": "bridge",
      "ports": {
        "8080/tcp": 8080
      },
      "binds": {
        "/var/run/docker.sock": {
          "bind": "/var/run/docker.sock",
          "mode": "rw"
        },
        "{PWD}/config": {
          "bind": "/supervisor/.config",
          "mode": "rw"
        }
      }
    },
    "companion-core": {
      "tag": "master",
      "image": "bluerobotics/companion-core",
      "enabled": true,
      "webui": false,
      "network": "host",
      "ports": {},
      "binds": {
        "/dev/" : {
          "bind": "/dev/",
          "mode": "rw"
        },
        "/var/run/wpa_supplicant/wlan0": {
          "bind": "/var/run/wpa_supplicant/wlan0",
          "mode": "rw"
        },
        "/tmp/wpa_playground": {
          "bind": "/tmp/wpa_playground",
          "mode": "rw"
        }
      },
      "privileged": true
    }
  }
}