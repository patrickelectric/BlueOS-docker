        },
        "binds": {
          "/var/run/wpa_supplicant/wlan0": {
            "bind": "/var/run/wpa_supplicant/wlan0",
            "mode": "rw"
          },
          "/tmp/wpa_playground": {
            "bind": "/tmp/wpa_playground",
            "mode": "rw"
          }
        },
        "privileged": true
